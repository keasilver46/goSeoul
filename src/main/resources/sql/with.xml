<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="withns">
    <insert id="insertCheck" parameterType="with">
        insert into withgo
        (with_no,user_no,with_id,with_nick,with_title,with_content,with_maxto,with_curno,with_tag,with_hit,
        with_category,with_age,with_start,with_end,with_date,with_filename)

        values
        (withgo_seq.nextval,#{user_no},#{with_id},#{with_nick},#{with_title},#{with_content},#{with_maxto},#{with_curno},#{with_tag},0,
        #{with_category},#{with_age},#{with_start},#{with_end},sysdate,#{with_filename, jdbcType=VARCHAR})
    </insert>
</mapper>